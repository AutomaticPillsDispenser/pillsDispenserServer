{
    "version": 2,
    "builds": [
      {
        "src": "src/index.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/middleware/(.*)",
        "dest": "src/middleware/$1"
      },
      {
        "src": "/routes/(.*)",
        "dest": "src/routes/$1"
      },
      {
        "src": "/config/(.*)",
        "dest": "src/config/$1"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.ts"
      }
    ]
  }
  